{
  "presets": [
    ["@babel/preset-env", { "modules": false, "loose": true }],
    "@babel/preset-react"
  ],
  "plugins": [
    ["@babel/plugin-proposal-class-properties", { "loose": true }],
  ],
  "env": {
    "rollup": {
      "plugins": [
        "babel-plugin-dev-expression"
      ]
    }
  }
}
